// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Projects{
    id Int @id @default(autoincrement())
    name String @unique
    studentName String
    abstract String
    imgURL String
    slug String

    keywords Keywords[]
}


model Keywords{
    id Int @id @default(autoincrement())
    name String @unique
    projects Projects[]
}